{
  "name": "hex-web",
  "version": "0.0.1",
  "description": "React front-end for Hexahedron.io",
  "main": "src/index.html",
  "scripts": {
    "start": "parcel src/index.html --out-dir build/dev",
    "dev-no-cache": "parcel src/index.html --out-dir build/dev --no-cache",
    "deploy": "parcel build src/index.html --out-dir build --no-minify",
    "deploy-prod": "parcel build src/index.html --out-dir build --no-source-maps"
  },
  "keywords": [
    "mtg",
    "mtg-cube"
  ],
  "author": "Michael liu",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-root-import": "^6.4.1",
    "babel-plugin-transform-import": "^1.5.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "parcel-bundler": "^1.12.3"
  },
  "dependencies": {
    "@aws-amplify/api": "^1.0.42",
    "@aws-amplify/auth": "^1.2.31",
    "@loadable/component": "^5.10.2",
    "@material-ui/core": "^4.3.3",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.3.3",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-async-hook": "^3.4.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4"
  }
}
