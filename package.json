{
  "name": "hex-web",
  "version": "0.0.1",
  "description": "React front-end for Hexahedron.io",
  "main": "src/index.html",
  "scripts": {
    "start": "parcel src/index.html --out-dir build/dev",
    "dev-no-cache": "parcel src/index.html --out-dir build/dev --no-cache",
    "deploy": "parcel build src/index.html --out-dir build --no-minify",
    "deploy-prod": "parcel build src/index.html --out-dir build --no-source-maps"
  },
  "keywords": [
    "mtg",
    "mtg-cube"
  ],
  "author": "Michael liu",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "babel-plugin-transform-import": "^1.5.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-externals": "^0.2.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4"
  },
  "dependencies": {
    "@aws-amplify/api": "^1.2.3",
    "@aws-amplify/auth": "^1.4.3",
    "@loadable/component": "^5.10.3",
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.5.0",
    "awesome-debounce-promise": "^2.1.0",
    "clsx": "^1.0.4",
    "idb": "^4.0.5",
    "prop-types": "^15.7.2",
    "react-async-hook": "^3.6.1",
    "react-beautiful-dnd": "^12.0.0-beta.12",
    "react-redux": "^7.1.1",
    "use-constant": "^1.0.0"
  },
  "externals": {
    "react": "React",
    "react-dom": "ReactDOM",
    "react-router-dom": "ReactRouterDOM",
    "redux": "Redux"
  }
}
